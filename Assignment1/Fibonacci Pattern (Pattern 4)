import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int row=sc.nextInt();
        int a=0;
        int b=1;
        
        for(int i=0;i<row;i++){
            for(int j=0;j<=i;j++){
               int sum=a+b;
               System.out.print(a+"\t");
               a=b;
               b=sum;
              
            }
            System.out.println();
        }
    }
}
