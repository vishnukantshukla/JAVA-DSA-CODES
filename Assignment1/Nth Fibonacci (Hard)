import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int a=0;
        int b=1;
        int term=sc.nextInt();
        int i=0;
        int sum;
        while(i<term){
            sum=a+b;
            a=b;
            b=sum;
            i++;
            
        }
       System.out.print(a);
       
    }
}
