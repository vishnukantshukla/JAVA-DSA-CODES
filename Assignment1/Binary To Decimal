import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int sum=0;
        int mul=1;
        while(num>0){
            int rem = num%10;
            sum=sum+mul*rem;
            num=num/10;
            mul=mul*2;
           
        }
        System.out.println(sum);

    }
}
