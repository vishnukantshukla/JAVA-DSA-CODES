import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int num=sc.nextInt();
        int sum=0;
        int mul=1;
        while(num>0){
            int rem=num%8;
            sum=sum+rem*mul;
            num/=8;
            mul=mul*10;


        }
        System.out.print(sum);
    }    
}
