import java.util.*;
public class Main {
    public static void main(String args[]) {
    Scanner sc=new Scanner(System.in);
    int a=sc.nextInt();
    int b=sc.nextInt();
    int count=0;
    int Lcm=0;
    for(int i=1;i<=a*b;i++){
        if(i%a==0 && i%b==0){
           Lcm=i;
           break;
        }
    }
    System.out.println(Lcm);
    }
    
}
