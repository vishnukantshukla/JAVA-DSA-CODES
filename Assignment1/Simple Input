import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int num=1;
        int count=0;
        while(num>0){
            int b=sc.nextInt();
            count=count+b;
            if(count<0){
                break;
            }
            num++;
            System.out.println(b);
        }
    }
}
    

    
