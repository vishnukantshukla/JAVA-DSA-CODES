import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner str = new Scanner(System.in);
        String st = str.next();
        for (int i = 0; i < st.length(); i++) {
            if (st.charAt(i) >= 'A' && st.charAt(i) <= 'Z') {
                System.out.println();

            }
            System.out.print(st.charAt(i));
        }
    }

}
